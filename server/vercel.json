{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "builds": [
    {
      "src": "./build/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["./build/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/server.js"
    }
  ]
}
